<h1>Login</h1>
<br />







<%= form_tag session_path do %>
  <fieldset>
    <p> 
      <label for "email"> E-mail</label>
      <%= text_field_tag :email, params[:name] %>
    </p>

    <p>
      <label for "password">Password</label>
      <%= password_field_tag :password, params[:password] %>
    </p>
    <p>
      <%= submit_tag "Log in" %>
    </p>
  </fieldset>
<% end %>